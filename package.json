{
  "name": "webrtc-troubleshoot",
  "version": "8.1.1",
  "description": "A way to add webrtc troubleshooting to your app",
  "main": "src/index.js",
  "license": "MIT",
  "config": {
    "report": "report --reporter=lcov --reporter=cobertura"
  },
  "scripts": {
    "build": "webpack",
    "lint": "semistandard",
    "lint:fix": "semistandard --fix",
    "unit-test": "nyc ava test/unit/*.js",
    "postunit-test": "nyc $npm_package_config_report",
    "unit-test-file": "ava FILE=process.env.file test/unit/${FILE}",
    "coverage": "nyc ava test/unit/*.js",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "test": "npm run lint && npm run unit-test"
  },
  "repository": "https://github.com/mypurecloud/webrtc-troubleshooter",
  "engines": {
    "node": ">= 4.4.0"
  },
  "devDependencies": {
    "ava": "^0.19.1",
    "babel-cli": "^6.22.2",
    "babel-preset-env": "^1.7.0",
    "babelify": "^7.2.0",
    "browser-env": "^2.0.31",
    "coveralls": "^3.0.2",
    "nyc": "^13.0.1",
    "semistandard": "^10.0.0",
    "sinon": "^2.2.0",
    "webrtc-adapter": "^6.1.5"
  },
  "dependencies": {
    "localmedia": "^4.0.0",
    "rtcpeerconnection": "^8.0.1",
    "webrtc-stats-gatherer": "^4.2.10"
  },
  "babel": {
    "presets": [
      "env"
    ]
  },
  "ava": {
    "files": [
      "!**/Ssim.js",
      "!**/StatisticsAggregate.js",
      "!**/VideoFrameChecker.js",
      "!**/WebrtcCall.js"
    ],
    "concurrency": 5,
    "failFast": true,
    "failWithoutAssertions": false,
    "tap": true,
    "powerAssert": false,
    "require": [
      "babel-register",
      "./test/helpers/setup-browser-env.js"
    ]
  },
  "semistandard": {
    "ignore": [
      "**/dist"
    ]
  }
}
